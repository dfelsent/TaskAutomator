<p>&nbsp;</p>
<h2>Previously Generated Podcast Codes</h2>
<p>&nbsp;</p>

<table>
    <th>Speaker</th>
    <th>Date</th>
<% @mycodes.each do |mycode| %>
    <tr>
        
<td><%= mycode.speaker %></td>
  <td><%= mycode.date.strftime('%B %d, %Y') %></td>
<td><%= link_to 'Show', mycode %></td>
<td><%= link_to 'Edit', edit_mycode_path(mycode) %></td>
 <td><%= link_to 'Delete', mycode, method: :delete %></td>
    </tr>
    <% end %>
</table>

<br />

<%= link_to 'Generate New Code', new_mycode_path %>&nbsp;&nbsp;&nbsp;<%= link_to 'Back to Home Page', start_index_path %>
